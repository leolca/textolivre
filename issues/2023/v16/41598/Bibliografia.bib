	issn = {1664-1078},
	shorttitle = {Masking {Emotions}},
	url = {https://www.frontiersin.org/articles/10.3389/fpsyg.2021.669432/full},
	doi = {10.3389/fpsyg.2021.669432},
	abstract = {To date, COVID-19 has spread across the world, changing our way of life and forcing us to wear face masks. This report demonstrates that face masks influence the human ability to infer emotions by observing facial configurations. Specifically, a mask obstructing a face limits the ability of people of all ages to infer emotions expressed by facial features, but the difficulties associated with the mask’s use are significantly pronounced in children aged between 3 and 5 years old. These findings are of essential importance, as they suggest that we live in a time that may potentially affect the development of social and emotion reasoning, and young children’s future social abilities should be monitored to assess the true impact of the use of masks.},
	language = {pt},
	urldate = {2021-11-11},
