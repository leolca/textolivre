@article{jimenez-hernandez_digital_2020,
	title = {Digital {Competence} of {Future} {Secondary} {School} {Teachers}: {Differences} {According} to {Gender}, {Age}, and {Branch} of {Knowledge}},
	volume = {12},
	issn = {2071-1050},
	shorttitle = {Digital {Competence} of {Future} {Secondary} {School} {Teachers}},
	url = {https://www.mdpi.com/2071-1050/12/22/9473},
	doi = {10.3390/su12229473},
	abstract = {The development of related technological skills in secondary students is perceived as unachievable if the teachers do not have enough technological expertise to guide their students. This study was based on investigating digital competence on the population of graduate students undertaking a Master’s degree in Education to train as teachers for the secondary educational level. The study made it possible to conclude, on the one hand, the homogeneity of university degrees within the scope of the Bologna Plan, with respect to mean levels of digital training. On the other hand, more exhaustively, differences have come to light concerning specific training on the different digital competence areas in the DigComp evaluation system, related to gender, branch of knowledge, age, and by considering the self-perception of individuals on their own capabilities in everyday technological issues. Consequently, the need for incorporating an ICT syllabus into the subjects covered by a Master’s in education has been highlighted, as well as promoting females, older people, and nontechnological degrees. Digital training on critical competence areas for teaching, such as the creation of digital content, suffers from major shortcomings and should also be promoted.},
	language = {en},
	number = {22},
	urldate = {2023-01-19},
	journal = {Sustainability},
	author = {Jiménez-Hernández, David and González-Calatayud, Víctor and Torres-Soto, Ana and Martínez Mayoral, Asunción and Morales, Javier},
	month = nov,
	year = {2020},
	pages = {9473},
}

