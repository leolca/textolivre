@article{mercader_resistencias_2019,
	title = {Las resistencias del profesorado universitario a la utilización de las tecnologías digitales},
	volume = {48},
	issn = {0210-2773, 2341-2313},
	url = {https://www.unioviedo.es/reunido/index.php/AA/article/view/13178},
	doi = {10.17811/rifie.48.2.2019.167-174},
	abstract = {El estudio desarrollado centra su foco de atención en el profesorado universitario y en los motivos por los cuales éstos no utilizan habitualmente las tecnologías digitales de manera habitual como herramienta de enseñanza-aprendizaje en el aula universitaria. El objetivo principal es identificar las resistencias del profesorado de la Facultad de Ciencias de la Educación de la Universidad Autónoma de Barcelona a la utilización de tecnologías digitales. Se utiliza una metodología mixta secuencial, con la técnica del cuestionario y entrevistas a profesores y gestores. Los resultados muestran el hábito, la gestión de la innovación, la falta de formación, el trabajo a reloj y la inversión en actualización como las resistencias principales a la utilización de las tecnologías digitales. En conclusión, se determina que la utilización de las tecnologías digitales es escasa, que su utilización no depende tanto de la edad o estabilidad laboral, sino de la predisposición y autopercepción del conocimiento de tecnologías digitales, así como que la falta de planificación y evaluación en la institucionalización del cambio es uno de las mayores barreras existentes.},
	number = {2},
	journal = {Aula Abierta},
	author = {Mercader, Cristina},
	month = may,
	year = {2019},
	pages = {167},
}

