@article{orosco_fabian_adolescentes_2020,
	title = {Adolescentes frente a los riesgos en el uso de las {TIC}},
	volume = {22},
	issn = {1607-4041},
	url = {https://redie.uabc.mx/redie/article/view/2298},
	doi = {10.24320/redie.2020.22.e17.2298},
	abstract = {Las Tecnologías de la Información y la Comunicación (TIC) han impactado en adolescentes de todos los sectores de la sociedad y utilizarlas sin orientación conlleva riesgos. Este estudio evalúa la frecuencia de los riesgos en el uso de las TIC en adolescentes de la región Junín (Perú). La muestra estuvo compuesta por 955 adolescentes con edades comprendidas entre los 13 y 19 años (M = 15.21, DT = 1.07) de diferentes provincias de la región Junín. La selección de participantes atendió al muestreo aleatorio estratificado. Se recogió información a través de un instrumento construido ad hoc. Los resultados muestran que existe diferencia significativa ({\textless} .000) en cuanto al conocimiento de riesgos, siendo mayor en mujeres. Los principales riesgos a los que se exponen son: acceso a contenidos inapropiados (46.2\%), sexting (24.9\%), cyberbullying (20.9\%), grooming (20.8\%), sexcasting (14.8\%) y sextorsión (12.3\%). Las diferencias fueron significativas entre la población de varones en la mayoría de los riesgos TIC que se analizaron. Se concluye que los adolescentes de esta región peruana están expuestos a estos riesgos y, por tanto, se debe orientar el uso responsable de las TIC.},
	number = {1},
	urldate = {2023-01-19},
	journal = {Revista Electrónica de Investigación Educativa},
	author = {Orosco Fabian, Jhon Richard and Pomasunco Huaytalla, Rocío},
	month = jun,
	year = {2020},
	pages = {1},
}

